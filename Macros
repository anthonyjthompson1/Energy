Sub Fix_EA()
    Sheets("Air-Side Outputs").Select
    Sheets("Results Summary").Visible = True
    Sheets("Results Summary").Select
    Sheets("Pump Energy").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Water-Side").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Exhaust Air Summary").Visible = True
    
    Sheets("Exhaust Air Summary").Select
    Cells.Select
    ActiveSheet.Unprotect
    Application.Iteration = True
    Application.Calculation = xlAutomatic
    
    Cells.Select
    Range("A:BW").Activate
    Selection.Replace What:="'C:\Users\athompson\Downloads\PSYCH.XLA'!", _
        Replacement:="", LookAt:=xlPart, SearchOrder:=xlByRows, MatchCase:= _
        False, SearchFormat:=False, ReplaceFormat:=False
    ActiveSheet.Protect DrawingObjects:=True, Contents:=True, Scenarios:=True
    Calculate

   Dim AI As Excel.AddIn
   Set AI = Application.AddIns.Add("P:\NYC\__OFFICE_STAFF\Thompson, Anthony\Calcs\PSYCH.XLA")
   AI.Installed = True
   Application.AddIns("Add_in").Installed = True
   
    Sheets("Results Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Range("CP49").Select
    ActiveCell.FormulaR1C1 = ""
    Sheets("Exhaust Air Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Water-Side").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Pump Energy").Select
    ActiveWindow.SelectedSheets.Visible = False

End Sub

