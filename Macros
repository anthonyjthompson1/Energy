Sub Hide_Unwanted()
    Sheets("Air-Side Outputs").Select
    Sheets("Results Summary").Visible = True
    Sheets("Results Summary").Select
    Sheets("Pump Energy").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Water-Side").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Exhaust Air Summary").Visible = True
    
    Application.Iteration = True
    Application.Calculation = xlAutomatic
    Sheets("Curves & Constants").Select
    Range("C25").Select
    Selection.Copy
    Range("A1").Select
    Selection.PasteSpecial Paste:=xlPasteValues, Operation:=xlNone, SkipBlanks _
        :=False, Transpose:=False
    Range("A1").Select
    Application.CutCopyMode = False
    Selection.ClearContents
    Application.Calculation = xlManual
    
        Sheets("Water-Side").Unprotect Password:="Thompson"
        Sheets("Pump Energy").Unprotect Password:="Thompson"
        Sheets("Exhaust Air Summary").Unprotect Password:="Thompson"
        
        Sheets("Water-Side").Select
        
        Range("Unwanted_001").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_002").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_003").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_004").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_005").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_006").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_007").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_008").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_009").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_010").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_011").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_012").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_013").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_014").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_015").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_016").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_017").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_018").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_019").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_020").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_021").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_022").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_023").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_024").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_025").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_026").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_027").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_028").Select
    Selection.EntireColumn.Hidden = True
        Range("Unwanted_029").Select
    Selection.EntireColumn.Hidden = True
'        Range("Unwanted_032").Select
'    Selection.EntireColumn.Hidden = True
    
        Sheets("Pump Energy").Select
        
        Range("Unwanted_030").Select
    Selection.EntireColumn.Hidden = True
    
        Sheets("Exhaust Air Summary").Select
        
        Range("Unwanted_031").Select
    Selection.EntireColumn.Hidden = True
    
    Sheets("Curves & Constants").Select
    
Dim curves_NA As Range

    For Each curves_NA In Range("dq9:ib9").Cells
        If curves_NA.Value = "N/A" Then
        curves_NA.EntireColumn.Hidden = True
        End If
    Next curves_NA
    For Each curves_NA In Range("dq9:ib9").Cells
        If curves_NA.Value <> "N/A" Then
        curves_NA.EntireColumn.Hidden = False
        End If
    Next curves_NA

    Sheets("Pump Energy").Select
        
Dim pump_NA As Range

    For Each pump_NA In Range("b12:kg12").Cells
        If pump_NA.Value = "N/A" Then
        pump_NA.EntireColumn.Hidden = True
        End If
    Next pump_NA
    For Each pump_NA In Range("b12:kg12").Cells
        If pump_NA.Value <> "N/A" Then
        pump_NA.EntireColumn.Hidden = False
        End If
    Next pump_NA
    
    Sheets("Water-Side").Select
        
Dim water_NA As Range

    For Each water_NA In Range("b7:ip7").Cells
        If water_NA.Value = "N/A" Then
        water_NA.EntireColumn.Hidden = True
        End If
    Next water_NA
    For Each water_NA In Range("b7:ip7").Cells
        If water_NA.Value <> "N/A" Then
        water_NA.EntireColumn.Hidden = False
        End If
    Next water_NA
    
    Sheets("Results Summary").Select
    
Dim results_NA As Range

    For Each results_NA In Range("a7:ET7").Cells
        If results_NA.Value = "N/A" Then
        results_NA.EntireColumn.Hidden = True
        End If
    Next results_NA
    For Each results_NA In Range("a7:ET7").Cells
        If results_NA.Value <> "N/A" Then
        results_NA.EntireColumn.Hidden = False
        End If
    Next results_NA
    
        Sheets("Curves & Constants").Select
        Range("C29").Select
        
    Sheets("Results Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Range("CP49").Select
    ActiveCell.FormulaR1C1 = ""
    Sheets("Exhaust Air Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Water-Side").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Pump Energy").Select
    ActiveWindow.SelectedSheets.Visible = False
        
        Sheets("Water-Side").Protect Password:="Thompson"
        Sheets("Pump Energy").Protect Password:="Thompson"
        Sheets("Exhaust Air Summary").Protect Password:="Thompson"
End Sub
Sub Open_CoGen()
        Sheets("Water-Side").Unprotect Password:="Thompson"
        Sheets("Pump Energy").Unprotect Password:="Thompson"
        Sheets("Exhaust Air Summary").Unprotect Password:="Thompson"
        
