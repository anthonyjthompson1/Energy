Sub Open_Boiler()

        Sheets("Water-Side").Unprotect Password:="Thompson"
        Sheets("Pump Energy").Unprotect Password:="Thompson"
        Sheets("Exhaust Air Summary").Unprotect Password:="Thompson"

    Sheets("Air-Side Outputs").Select
    Sheets("Results Summary").Visible = True
    Sheets("Results Summary").Select
    Sheets("Pump Energy").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Water-Side").Visible = True
    Sheets("Pump Energy").Select
    Sheets("Exhaust Air Summary").Visible = True

    If Range("Boiler_Button").Value = "Yes" Then
        
        Sheets("Water-Side").Select
        
        Range("Boiler_001").Select
    Selection.EntireColumn.Hidden = True
       Range("Boiler_002").Select
    Selection.EntireColumn.Hidden = True
       Range("Boiler_003").Select
    Selection.EntireColumn.Hidden = True
       Range("Boiler_004").Select
    Selection.EntireColumn.Hidden = True
    
        Sheets("Pump Energy").Select
    
       Range("Boiler_005").Select
    Selection.EntireColumn.Hidden = True
       Range("Boiler_006").Select
    Selection.EntireColumn.Hidden = True
    
        Sheets("Curves & Constants").Select
        
       Range("Boiler_007").Select
    Selection.EntireColumn.Hidden = True
       Range("Boiler_008").Select
    Selection.EntireColumn.Hidden = True
    
        Range("Boiler_Button").Select
    ActiveCell.FormulaR1C1 = "No"
    ActiveWindow.SmallScroll Down:=-100
    
    Sheets("Results Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Range("CP49").Select
    ActiveCell.FormulaR1C1 = ""
    Sheets("Exhaust Air Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Water-Side").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Pump Energy").Select
    ActiveWindow.SelectedSheets.Visible = False

Else
        Sheets("Water-Side").Select
        
        Range("Boiler_001").Select
    Selection.EntireColumn.Hidden = False
       Range("Boiler_002").Select
    Selection.EntireColumn.Hidden = False
       Range("Boiler_003").Select
    Selection.EntireColumn.Hidden = False
       Range("Boiler_004").Select
    Selection.EntireColumn.Hidden = False
    
        Sheets("Pump Energy").Select
    
       Range("Boiler_005").Select
    Selection.EntireColumn.Hidden = False
       Range("Boiler_006").Select
    Selection.EntireColumn.Hidden = False
    
        Sheets("Curves & Constants").Select
        
       Range("Boiler_007").Select
    Selection.EntireColumn.Hidden = False
       Range("Boiler_008").Select
    Selection.EntireColumn.Hidden = False
    
        Range("Boiler_Button").Select
    ActiveCell.FormulaR1C1 = "Yes"
    ActiveWindow.SmallScroll Down:=-100
    
    Sheets("Results Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Range("CP49").Select
    ActiveCell.FormulaR1C1 = ""
    Sheets("Exhaust Air Summary").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Water-Side").Select
    ActiveWindow.SelectedSheets.Visible = False
    Sheets("Pump Energy").Select
    ActiveWindow.SelectedSheets.Visible = False
    
        Sheets("Water-Side").Protect Password:="Thompson"
        Sheets("Pump Energy").Protect Password:="Thompson"
        Sheets("Exhaust Air Summary").Protect Password:="Thompson"
End If
End Sub

